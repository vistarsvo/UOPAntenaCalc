                        </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="Помощь">
                        <items>
                              <MenuItem mnemonicParsing="false" text="Автор" />
                          <MenuItem mnemonicParsing="false" text="О программе" />
                        </items>
                      </Menu>
                    </menus>
                  </MenuBar>
               </top>
               <bottom>
                  <ToolBar prefHeight="40.0" BorderPane.alignment="CENTER">
                    <items>
                      <Button mnemonicParsing="false" text="Провести расчет" />
                        <ProgressBar progress="0.21">
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                        </ProgressBar>
                    </items>
                  </ToolBar>
               </bottom>
               <center>
                  <TabPane minHeight="300.0" minWidth="-Infinity" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
                    <tabs>
                      <Tab text="Данные для расчета">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                        </content>
                      </Tab>
                      <Tab text="Просмотр расчетов">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                        </content>
                      </Tab>
                        <Tab text="Графики">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                          </content>
                        </Tab>
                        <Tab text="Таблица 1">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="372.0" prefWidth="567.0" />
