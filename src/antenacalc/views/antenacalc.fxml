<GridPane alignment="center" hgap="10" stylesheets="@DarkTheme.css" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="antenacalc.controllers.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity">
         <children>
            <BorderPane>
               <top>
                  <MenuBar BorderPane.alignment="TOP_LEFT">
                    <menus>
                      <Menu mnemonicParsing="false" text="Главное">
                        <items>
                              <MenuItem mnemonicParsing="false" text="Провести расчет" />
                          <MenuItem mnemonicParsing="false" text="Закрыть" />
                        </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="Данные">
                        <items>
                              <MenuItem mnemonicParsing="false" text="График" />
